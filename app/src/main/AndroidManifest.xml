<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="cn.edu.bistu.cs.se.intentfiler14">

<application android:allowBackup="true" android:icon="@mipmap/ic_launcher" android:label="@string/app_name"
    android:supportsRtl="true" android:theme="@style/AppTheme">
<activity android:name=".MainActivity">
    <intent-filter>
        <action android:name="android.intent.action.MAIN" />//action用来指定想要执行的动作名称

        <category android:name="android.intent.category.LAUNCHER" />
        //Category为Intent提供了一个附加的方法来标识组件能够被启动和执行。

    </intent-filter>
</activity>
<activity android:name="cn.edu.bistu.cs.se.intentfiler14.Main2Activity"
    android:label="@string/title_activity_main2"
    android:theme="@style/AppTheme.NoActionBar">
    <intent-filter>
        <action android:name="startAnotherActivity" />//action用来指定想要执行的动作名称

        <category android:name="android.intent.category.DEFAULT" />
        //Category为Intent提供了一个附加的方法来标识组件能够被启动和执行。

    </intent-filter>

</activity>

    </application>

</manifest>


